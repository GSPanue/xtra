{
  "name": "xtra",
  "description": "A web and native application for looking for after school classes and activities",
  "author": "Gurdev S. Panue",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "run-p install:*",
    "install:web": "cd web && npm install",
    "install:native": "cd native && npm install",
    "build:web": "cd web && npm run build",
    "start:web": "cd web && npm start",
    "start:native": "cd native && tns run android"
  },
  "dependencies": {
    "npm-run-all": "4.1.5"
  }
}
